#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/ngrok@5.0.0-beta.2/node_modules/ngrok/bin/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/ngrok@5.0.0-beta.2/node_modules/ngrok/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/ngrok@5.0.0-beta.2/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/ngrok@5.0.0-beta.2/node_modules/ngrok/bin/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/ngrok@5.0.0-beta.2/node_modules/ngrok/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/ngrok@5.0.0-beta.2/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/ngrok"   "$@"
exit $?
