#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1277e6357ccdbfa98995ccfb3a2f3ff2/node_modules/vite/bin/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1277e6357ccdbfa98995ccfb3a2f3ff2/node_modules/vite/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1277e6357ccdbfa98995ccfb3a2f3ff2/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1277e6357ccdbfa98995ccfb3a2f3ff2/node_modules/vite/bin/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1277e6357ccdbfa98995ccfb3a2f3ff2/node_modules/vite/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1277e6357ccdbfa98995ccfb3a2f3ff2/node_modules:/mnt/h/Code/Learn/calorie-tracker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
